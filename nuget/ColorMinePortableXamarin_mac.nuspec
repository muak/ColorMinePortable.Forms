<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
    <metadata>
        <id>ColorMinePortable.Forms</id>
        <version>1.0.1</version>
        <title>ColorMinePortable for Xamarin.Forms</title>
        <authors>kamu</authors>
        <owners>kamu</owners>
        <projectUrl>https://github.com/muak/ColorMinePortable.Forms</projectUrl>
        <licenseUrl>https://github.com/muak/ColorMinePortable.Forms/blob/master/LICENSE.txt</licenseUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Color conversion portable class library for Xamarin.Forms</description>
        <summary>
            ## Supported Color Models:
            * CMY
            * CMYK
            * HSL
            * HSB
            * HSV
            * CIE L*AB
            * Hunter LAB
            * LCH
            * LUV
            * sRGB
            * XYZ
            * YXY
            * Munsell
            * Hex
            * Xamarin.Forms.Color
        </summary>
        <language>en-US</language>
        <dependencies>
            <group targetFramework="portable-win+net45+wp80+win81+wpa81+MonoAndroid10+MonoTouch10+Xamarin.iOS10">
                <dependency id="Xamarin.Forms" version="2.3.2" />
				<dependency id="ColorMinePortable" version="1.0.1" />
            </group>
            <group targetFramework="MonoAndroid">
                <dependency id="Xamarin.Forms" version="2.3.2" />
				<dependency id="ColorMinePortable" version="1.0.1" />
            </group>
            <group targetFramework="MonoTouch">
                <dependency id="Xamarin.Forms" version="2.3.2" />
				<dependency id="ColorMinePortable" version="1.0.1" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <!-- PCL -->
        <file src="../ColorMinePortable.Forms/bin/Release/ColorMinePortable.Forms.dll" target="lib/portable-win+net45+wp80+win81+wpa81+MonoAndroid10+MonoTouch10+Xamarin.iOS10" />
